{
	"name": "Theme-MarkdownKit",
	"displayName": "Markdown Theme Kit",
	"version": "0.0.1",
	"private": false,
	"description": "Theme Kit for VS Code optimized for Markdown. Based on the TextMate themes.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"scripts": {
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"contributes": {
		"themes": [
			{
				"label": "Markdown Editor",
				"path": "./themes/MarkdownEditor.tmTheme",
				"uiTheme": "vs"
			},
			{
				"label": "Markdown Editor Dark",
				"path": "./themes/MarkdownEditor-Dark.tmTheme",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Markdown Editor Focus",
				"path": "./themes/MarkdownEditor-Focus.tmTheme",
				"uiTheme": "vs"
			},
			{
				"label": "Markdown Editor Yellow",
				"path": "./themes/MarkdownEditor-Yellow.tmTheme",
				"uiTheme": "vs"
			}
		]
	},
	"devDependencies": {
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "icon.png"
}
