{
	"name": "Theme-MarkdownKit",
	"displayName": "Markdown Theme Kit",
	"description": "Theme Kit for VS Code optimized for Markdown. Based on the TextMate themes.",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"themes": [
			{
				"label": "Markdown Editor",
				"path": "./themes/MarkdownEditor.tmTheme",
				"uiTheme": "vs"
			},
			{
				"label": "Markdown Editor Dark",
				"path": "./themes/MarkdownEditor-Dark.tmTheme",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Markdown Editor Focus",
				"path": "./themes/MarkdownEditor-Focus.tmTheme",
				"uiTheme": "vs"
			},
			{
				"label": "Markdown Editor Yellow",
				"path": "./themes/MarkdownEditor-Yellow.tmTheme",
				"uiTheme": "vs"
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.7"
	},
	"icon": "icon.png"
}
