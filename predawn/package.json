{
	"name": "Theme-PredawnKit",
	"displayName": "Predawn Theme Kit",
	"description": "Predawn themes for VS Code. Based on the TextMate themes.",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"themes": [
			{
				"label": "Predawn",
				"path": "./themes/predawn.tmTheme",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Predawn GF Markdown",
				"path": "./themes/predawn-gf-markdown.tmTheme",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Predawn Markdown",
				"path": "./themes/predawn-markdown.tmTheme",
				"uiTheme": "vs-dark"
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.7"
	},
	"icon": "icon.png"
}
